        <input>
			<condition>
				<and>
					<equals>
						<property>/controls/engines/thrust-limit</property>
						<value>FLX</value>
					</equals>
					<equals>
						<property>/systems/thrust/state1</property>
						<value>MCT</value>
					</equals>
				</and>
			</condition>
            <expression>
                <table>
					<product>
						<value>0.97</value>
						<property>/engines/flx-throttle</property>
					</product>
                    <entry><ind>0.00</ind><dep> 22.4</dep></entry>
                    <entry><ind>1.00</ind><dep>103.8</dep></entry>
                </table>
            </expression>
        </input>
        <input>
